@model IEnumerable<Application.eGreeting.Models.Card>

@{
    ViewBag.Title = "Search";
}

<h2>Search</h2>



<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
            <div class="box-card">
                <img src="~/ImageCard/@item.ImageName" alt="image" class="image" style="width:100%">
                <div class="middle">
                    <a href="#" class="btn-select">SELECT</a>
                </div>
                <div class="name-card">
                    <h5>@Html.DisplayFor(modelItem => item.NameCard)</h5>
                </div>
            </div>
        </div>
    }

</div>

